#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5c0bd7d14fb0 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v0x5c0bd7d27eb0_0 .var "a", 0 0;
v0x5c0bd7d27f80_0 .var "b", 0 0;
v0x5c0bd7d28050_0 .net "c", 0 0, L_0x5c0bd7d28150;  1 drivers
S_0x5c0bd7d15140 .scope module, "uut" "and_gate" 2 6, 3 1 0, S_0x5c0bd7d14fb0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "c";
L_0x5c0bd7d28150 .functor AND 1, v0x5c0bd7d27eb0_0, v0x5c0bd7d27f80_0, C4<1>, C4<1>;
v0x5c0bd7cdc0f0_0 .net "a", 0 0, v0x5c0bd7d27eb0_0;  1 drivers
v0x5c0bd7d27cd0_0 .net "b", 0 0, v0x5c0bd7d27f80_0;  1 drivers
v0x5c0bd7d27d90_0 .net "c", 0 0, L_0x5c0bd7d28150;  alias, 1 drivers
    .scope S_0x5c0bd7d14fb0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c0bd7d27eb0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c0bd7d27f80_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c0bd7d27eb0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c0bd7d27f80_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c0bd7d27eb0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c0bd7d27f80_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c0bd7d27eb0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c0bd7d27f80_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x5c0bd7d14fb0;
T_1 ;
    %vpi_call 2 23 "$monitor", "At time %t, a = %b, b = %b, c = %b", $time, v0x5c0bd7d27eb0_0, v0x5c0bd7d27f80_0, v0x5c0bd7d28050_0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x5c0bd7d14fb0;
T_2 ;
    %vpi_call 2 28 "$dumpfile", "dump_and_gate.vcd" {0 0 0};
    %vpi_call 2 29 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5c0bd7d14fb0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test/testbench_and_gate.v";
    "src/and_gate.v";
